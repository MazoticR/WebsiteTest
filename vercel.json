{
  "version": 2,
  "builds": [
    {
      "src": "api/proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [

    {
      "src": "/api/(shipments|pick_tickets)(/.*)?",
      "dest": "/api/proxy.js"
    },
    

    {
      "src": "/lekker/tools/box-labels/css/(.*)",
      "headers": { "Content-Type": "text/css" },
      "dest": "/public/lekker/tools/box-labels/css/$1"
    },
    {
      "src": "/lekker/tools/box-labels/js/(.*)",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/public/lekker/tools/box-labels/js/$1"
    },
    {
      "src": "/lekker/tools/box-labels(/.*)?",
      "dest": "/public/lekker/tools/box-labels/index.html"
    },
    

    {
      "src": "/lekker/tools/packing-list/css/(.*)",
      "headers": { "Content-Type": "text/css" },
      "dest": "/public/lekker/tools/packing-list/css/$1"
    },
    {
      "src": "/lekker/tools/packing-list/js/(.*)",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/public/lekker/tools/packing-list/js/$1"
    },
    {
      "src": "/lekker/tools/packing-list(/.*)?",
      "dest": "/public/lekker/tools/packing-list/index.html"
    },
    

    {
      "src": "/lekker(/.*)?",
      "dest": "/public/lekker/index.html",
      "has": [
        {
          "type": "host",
          "value": "lekker.aglcodes.com"
        }
      ]
    },
    

    {
      "src": "/",
      "status": 308,
      "headers": {
        "Location": "https://www.aglcodes.com"
      },
      "has": [
        {
          "type": "host",
          "value": "aglcodes.com"
        }
      ]
    },
    

    {
      "src": "/(.*)",
      "dest": "/public/personal/index.html",
      "has": [
        {
          "type": "host",
          "value": "www.aglcodes.com"
        }
      ]
    }
  ]
}
