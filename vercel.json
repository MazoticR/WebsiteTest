{
  "version": 2,
  "builds": [
    {
      "src": "api/proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [

    {
      "src": "/api/(.*)",
      "dest": "/api/proxy.js"
    },
    

    {
      "src": "/tools/box-labels",
      "dest": "/public/lekker/tools/box-labels/index.html",
      "has": [{ "type": "host", "value": "lekker.aglcodes.com" }]
    },
    {
      "src": "/tools/box-labels/(.*)",
      "dest": "/public/lekker/tools/box-labels/index.html",
      "has": [{ "type": "host", "value": "lekker.aglcodes.com" }]
    },
    

    {
      "src": "/tools/packing-list",
      "dest": "/public/lekker/tools/packing-list/index.html",
      "has": [{ "type": "host", "value": "lekker.aglcodes.com" }]
    },
    {
      "src": "/tools/packing-list/(.*)",
      "dest": "/public/lekker/tools/packing-list/index.html",
      "has": [{ "type": "host", "value": "lekker.aglcodes.com" }]
    },
    

    {
      "src": "/tools/(box-labels|packing-list)/css/(.*)",
      "headers": { "Content-Type": "text/css" },
      "dest": "/public/lekker/tools/$1/css/$2"
    },
    {
      "src": "/tools/(box-labels|packing-list)/js/(.*)",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/public/lekker/tools/$1/js/$2"
    },
    

    {
      "src": "/",
      "dest": "/public/lekker/index.html",
      "has": [{ "type": "host", "value": "lekker.aglcodes.com" }]
    },
    

    {
      "src": "/",
      "status": 308,
      "headers": { "Location": "https://www.aglcodes.com" },
      "has": [{ "type": "host", "value": "aglcodes.com" }]
    },
    

    {
      "src": "/(.*)",
      "dest": "/public/personal/index.html",
      "has": [{ "type": "host", "value": "www.aglcodes.com" }]
    }
  ]
}
