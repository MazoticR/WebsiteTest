{
  "version": 2,
  "builds": [
    {
      "src": "api/proxy.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api/proxy.js"
    },
    {
      "src": "/notion-embed",
      "dest": "/api/proxy.js"
    },
    {
      "src": "/",
      "status": 308,
      "headers": {
        "Location": "https://www.aglcodes.com"
      },
      "has": [
        {
          "type": "host",
          "value": "aglcodes.com"
        }
      ]
    },
    {
      "src": "/(.*)",
      "dest": "/public/lekker/index.html",
      "has": [
        {
          "type": "host",
          "value": "lekker.aglcodes.com"
        }
      ]
    },
    {
      "src": "/(.*)",
      "dest": "/public/personal/index.html",
      "has": [
        {
          "type": "host",
          "value": "www.aglcodes.com"
        }
      ]
    }
  ]
}
