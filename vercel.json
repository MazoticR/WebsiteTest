{
  "version": 2,
  "builds": [
    {
      "src": "api/proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [

    {
      "src": "/api/(.*)",
      "dest": "/api/proxy.js"
    },
    

    {
      "src": "/lekker/tools/([^/]+)/(css|js)/(.*)",
      "headers": { "Content-Type": "$2/text/css" },
      "dest": "/public/lekker/tools/$1/$2/$3"
    },
    

    {
      "src": "/lekker/tools/purchase-orders",
      "dest": "/public/lekker/tools/purchase-orders/index.html"
    },
    
    {
      "src": "/lekker/tools/(box-labels|packing-list)",
      "dest": "/public/lekker/tools/$1/index.html"
    },
    

    {
      "src": "/lekker",
      "dest": "/public/lekker/index.html"
    },
    

    {
      "src": "/",
      "dest": "/public/lekker/index.html",
      "has": [
        {
          "type": "host",
          "value": "lekker\\.aglcodes\\.com"
        }
      ]
    },
    {
      "src": "/",
      "status": 308,
      "headers": { "Location": "https://www.aglcodes.com" },
      "has": [
        {
          "type": "host",
          "value": "aglcodes\\.com"
        }
      ]
    },
    {
      "src": "/(.*)",
      "dest": "/public/personal/index.html",
      "has": [
        {
          "type": "host",
          "value": "www\\.aglcodes\\.com"
        }
      ]
    }
  ]
}