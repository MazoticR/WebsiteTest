{
  "version": 2,
  "builds": [
    {
      "src": "api/proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/lekker/index.html",
      "has": [
        {
          "type": "host",
          "value": "(.*\\.vercel\\.app)"
        }
      ]
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/proxy.js"
    },
    

    {
      "src": "/lekker/tools/purchase-orders/css/(.*)",
      "headers": { "Content-Type": "text/css" },
      "dest": "/public/lekker/tools/purchase-orders/css/$1"
    },
    {
      "src": "/lekker/tools/purchase-orders/js/(.*)",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/public/lekker/tools/purchase-orders/js/$1"
    },
    {
      "src": "/lekker/tools/purchase-orders",
      "dest": "/public/lekker/tools/purchase-orders/index.html"
    },

    {
      "src": "/lekker/tools/box-labels/css/(.*)",
      "headers": { "Content-Type": "text/css" },
      "dest": "/public/lekker/tools/box-labels/css/$1"
    },
    {
      "src": "/lekker/tools/box-labels/js/(.*)",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/public/lekker/tools/box-labels/js/$1"
    },
    {
      "src": "/lekker/tools/packing-list/css/(.*)",
      "headers": { "Content-Type": "text/css" },
      "dest": "/public/lekker/tools/packing-list/css/$1"
    },
    {
      "src": "/lekker/tools/packing-list/js/(.*)",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/public/lekker/tools/packing-list/js/$1"
    },
    

    {
      "src": "/lekker/tools/(box-labels|packing-list)",
      "dest": "/public/lekker/tools/$1/index.html",
      "has": [
        {
          "type": "host",
          "value": "(lekker\\.aglcodes\\.com|.*\\.vercel\\.app)"
        }
      ]
    },
    {
      "src": "/lekker",
      "dest": "/public/lekker/index.html",
      "has": [
        {
          "type": "host",
          "value": "(lekker\\.aglcodes\\.com|.*\\.vercel\\.app)"
        }
      ]
    },
    {
      "src": "/",
      "status": 308,
      "headers": { "Location": "https://www.aglcodes.com" },
      "has": [
        {
          "type": "host",
          "value": "aglcodes\\.com"
        }
      ]
    },
    {
      "src": "/(.*)",
      "dest": "/public/personal/index.html",
      "has": [
        {
          "type": "host",
          "value": "www\\.aglcodes\\.com"
        }
      ]
    }
  ]
}