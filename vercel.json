{

  
"headers": [
    {
      "source": "/(.*)",
      "headers": [
        { 
          "key": "Content-Security-Policy", 
          "value": "frame-ancestors 'self' notion.so *.notion.so" 
        }
      ]
    }
  ],

  "routes": [
    { "src": "/api/(.*)", "dest": "/api/proxy.js" },
    { "src": "/tools/packing-list", "dest": "/public/tools/packing-list/index.html" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
