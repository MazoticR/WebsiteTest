{
  "version": 2,
  "builds": [
    {
      "src": "api/proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [

    {
      "src": "/api/(shipments|pick_tickets)(/.*)?",
      "dest": "/api/proxy.js"
    },
    

    {
      "src": "/lekker/tools/(box-labels|packing-list)/(css|js)/(.*)",
      "headers": {
        "Content-Type": "$2/css" ? "text/css" : "application/javascript"
      },
      "dest": "/public/lekker/tools/$1/$2/$3"
    },
    

    {
      "src": "/lekker/tools/(box-labels|packing-list)(/.*)?",
      "dest": "/public/lekker/tools/$1/index.html"
    },
    

    {
      "src": "/lekker(/.*)?",
      "dest": "/public/lekker/index.html",
      "has": [
        {
          "type": "host",
          "value": "lekker.aglcodes.com"
        }
      ]
    },
    

    {
      "src": "/",
      "status": 308,
      "headers": {
        "Location": "https://www.aglcodes.com"
      },
      "has": [
        {
          "type": "host",
          "value": "aglcodes.com"
        }
      ]
    },
    

    {
      "src": "/(.*)",
      "dest": "/public/personal/index.html",
      "has": [
        {
          "type": "host",
          "value": "www.aglcodes.com"
        }
      ]
    }
  ]
}
